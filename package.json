{
  "name": "arare",
  "version": "0.1.0",
  "description": "Lightweight auto-curried functional programming library",
  "license": "MIT",
  "repository": "klauscfhq/arare",
  "author": {
    "name": "Klaus Sinani",
    "email": "klauscfhq@protonmail.com",
    "url": "https://klauscfhq.github.io"
  },
  "bin": {
    "arare": "repl.js"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "index.js",
    "repl.js",
    "src"
  ],
  "keywords": [
    "functional",
    "javascript",
    "library",
    "programming"
  ],
  "scripts": {
    "lint": "xo",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html"
  },
  "devDependencies": {
    "ava": "1.0.0-beta.8",
    "jest": "^23.6.0",
    "xo": "*"
  },
  "xo": {
    "rule": {
      "curly": 0,
      "max-params": 1,
      "no-use-extend-native/no-use-extend-native": 1
    },
    "semicolon": false,
    "space": 2,
    "overrides": [
      {
        "files": "test/*.js",
        "globals": [
          "describe",
          "test",
          "expect"
        ]
      }
    ]
  }
}
